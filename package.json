{
  "name": "yonkenella-server",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts",
    "start:prod": "yarn build:prod && node ./dist/server.js",
    "build:prod": "tsc && cp -r ./ui ./dist/"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
